pipeline{
    agent any
    stages {
        stage('Build') {
            steps {
              echo "Here we will build our service! :)"
            }
        }
        stage('Test') {
            steps {
              echo "Here we will unit test our service! :)"
            }
        }
        stage('Release') {
            steps {
              echo "Here we will release our service! :)"
            }
        }
    }
}